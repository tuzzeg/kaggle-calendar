{
  "version": 1,
  "disable_existing_loggers": false,
  "formatters": {
    "simple": {
      "format": "%(asctime)s - %(name)s - %(levelname)s - %(message)s"
    }
  },
  "handlers": {
    "file": {
      "class": "logging.FileHandler",
      "level": "INFO",
      "formatter": "simple",
      "filename": "/app/d/python.DEBUG"
    }
  },
  "root": {
    "level": "INFO",
    "handlers": ["file"]
  },
  "googleapiclient.discovery": {
    "level": "ERROR",
    "handlers": ["file"]
  }
}
